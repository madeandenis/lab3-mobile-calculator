<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:type="radial"
                android:gradientRadius="70%p"
                android:centerX="0.5"
                android:centerY="0.5"
                android:startColor="@color/pink"
                android:centerColor="@color/light_teal"
                android:endColor="@color/light_green" />
        </shape>
    </item>
    <item android:top="0dp" android:left="0dp" android:right="0dp" android:bottom="0dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/semi_transparent_white" />
        </shape>
    </item>
</layer-list>
